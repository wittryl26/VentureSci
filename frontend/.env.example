# Frontend environment (copy to .env.local)

# Supabase project config (browser-safe)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# API and app URLs
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_APP_URL=http://localhost:3000

# OAuth providers configured in Supabase
NEXT_PUBLIC_OAUTH_GOOGLE_CLIENT_ID=your-google-client-id
NEXT_PUBLIC_OAUTH_GITHUB_CLIENT_ID=your-github-client-id

# Encrypted chat (public half of the keypair; safe for browser)
NEXT_PUBLIC_ENCRYPTED_CHAT_PUBLIC_KEY=-----BEGIN PUBLIC KEY-----\n...\n-----END PUBLIC KEY-----

# Optional analytics/observability
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=
NEXT_PUBLIC_SENTRY_DSN=
