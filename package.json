{
  "name": "venturesci",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/mobile",
    "packages/api"
  ],
  "scripts": {
    "lint": "npm run lint:api && npm run lint:mobile",
    "lint:api": "npm run lint --workspace @venturesci/api",
    "lint:mobile": "npm run lint --workspace @venturesci/mobile",
    "type-check": "npm run type-check:api && npm run type-check:mobile",
    "type-check:api": "npm run type-check --workspace @venturesci/api",
    "type-check:mobile": "npm run type-check --workspace @venturesci/mobile",
    "test": "npm run test:api && npm run test:mobile",
    "test:api": "npm test --workspace @venturesci/api",
    "test:mobile": "npm test --workspace @venturesci/mobile"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.3",
    "typescript": "5.5.4"
  }
}
